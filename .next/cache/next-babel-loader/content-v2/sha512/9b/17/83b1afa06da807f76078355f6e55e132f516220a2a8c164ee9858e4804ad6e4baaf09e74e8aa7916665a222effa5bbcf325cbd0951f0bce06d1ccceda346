{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\n//基本的には<LessonList/>と同じなのでそちらを参照\n//.whereでレッスン作成者とReduxのemailが一致するデータを参照\nimport React, { useState } from \"react\";\nimport firebase from \"firebase\";\nimport \"firebase/storage\";\nimport MyLesson from \"./MyLesson\";\nimport { connect } from \"react-redux\";\nimport Lib from \"../../Lib/address_lib\";\nimport Link from \"next/link\";\nimport { makeStyles } from '@material-ui/styles';\nimport Box from '@material-ui/core/Box';\nimport Typography from '@material-ui/core/Typography';\nimport List from '@material-ui/core/List';\nimport Button from '@material-ui/core/Button';\nimport Icon from '@material-ui/core/Icon';\nconst mylessonlist = makeStyles({\n  list: {\n    margin: '20px'\n  },\n  addbtn: {\n    textAlign: 'center'\n  },\n  btn: {\n    backgroundColor: \"#4A8\",\n    color: \"white\"\n  }\n});\n\nfunction MyLessonList(props) {\n  const {\n    0: items,\n    1: setItems\n  } = useState(\"no item\");\n\n  const getFireData = async () => {\n    const db = firebase.firestore();\n    const lessondata = [];\n    const lessonid = [];\n    const lessonitems = [];\n    const email = Lib.encodeEmail(props.email);\n    await db.collection(\"lessons\").where(\"createrid\", \"==\", email).get().then(function (querySnapshot) {\n      querySnapshot.forEach(function (doc) {\n        lessondata.unshift(doc.data());\n        lessonid.unshift(doc.id);\n      });\n\n      for (let i in lessonid) {\n        let id = lessonid[i];\n        let name = lessondata[i].lessonname;\n        let place = lessondata[i].lessonplace;\n        let time = lessondata[i].lessontime;\n        let text = lessondata[i].lessontext;\n        let price = lessondata[i].lessonprice;\n        lessonitems.push( /*#__PURE__*/_jsx(MyLesson, {\n          lessonid: id,\n          name: name,\n          place: place,\n          time: time,\n          text: text,\n          price: price\n        }));\n      }\n\n      setItems(lessonitems);\n    });\n  };\n\n  if (items == \"no item\") {\n    getFireData();\n  }\n\n  const classes = mylessonlist();\n  return /*#__PURE__*/_jsxs(\"div\", {\n    className: classes.list,\n    children: [/*#__PURE__*/_jsx(\"div\", {\n      className: classes.list,\n      children: /*#__PURE__*/_jsx(Box, {\n        color: \"white\",\n        bgcolor: \"#4A8\",\n        fontWeight: \"bold\",\n        p: 1,\n        children: /*#__PURE__*/_jsx(Typography, {\n          variant: \"h8\",\n          children: \"\\u6295\\u7A3F\\u3057\\u305F\\u30EC\\u30C3\\u30B9\\u30F3\"\n        })\n      })\n    }), items, /*#__PURE__*/_jsxs(\"div\", {\n      className: classes.addbtn,\n      children: [/*#__PURE__*/_jsx(Link, {\n        href: \"/lesson_add\",\n        children: /*#__PURE__*/_jsx(Button, {\n          variant: \"contained\",\n          size: \"large\",\n          startIcon: /*#__PURE__*/_jsx(Icon, {\n            children: \"add_circle\"\n          }),\n          className: classes.btn,\n          children: \"\\u30EC\\u30C3\\u30B9\\u30F3\\u8FFD\\u52A0\"\n        })\n      }), /*#__PURE__*/_jsx(\"button\", {\n        onClick: getFireData,\n        children: \"\\u691C\\u8A3C\"\n      })]\n    })]\n  });\n}\n\nMyLessonList = connect(state => state)(MyLessonList);\nexport default MyLessonList;","map":null,"metadata":{},"sourceType":"module"}