ync()=>{\r\n      const email = Lib.encodeEmail(props.email);\r\n      await db.collection(\"lessons\").doc(router.query.lessonid).set({\r\n      buyerid:email\r\n    },{merge: true})\r\n    }\r\n\r\n    if(lessonname ==\"\"){\r\n      getLessonData();\r\n    }\r\n\r\n    const classes = plofile();\r\n\r\n    // return(\r\n    //   <div>\r\n    //     <h1>レッスン情報</h1>\r\n    //     <Link as={`/message/${router.query.lessonid}`}\r\n    //           href=\"/messge/[lessonid]\">\r\n    //       <button onClick={dobuy}>購入</button>\r\n    //     </Link>\r\n    //     <h2>{lessonname}</h2>\r\n    //     <p>料金 : {price}</p>\r\n    //     <p>場所 : {place}</p>\r\n    //     <p>時間 : {time}</p>\r\n    //     <p>詳細 : {lessoncomment}</p>\r\n    //     <div className=\"lessonprofile\">\r\n    //     <Profile  username={profileusername} introduction={profileintroduction}/>\r\n    //     </div>\r\n    //   </div>\r\n    // );\r\n      return(\r\n        <div style={{marginTop:\"30px\"}}>\r\n                \r\n                <Typography variant=\"h6\" className={classes.title}>「レッスン名」</Typography>\r\n                <Button className = {classes.buybtn} size=\"large\" variant=\"outlined\">購入</Button>\r\n\r\n                <Paper elevation={24} rounded>\r\n                <Avatar aria-label=\"recipe\" className={classes.avatar}>\r\n                </Avatar>\r\n                \r\n                    <div className={classes.infolist}>\r\n                        <Typography variant=\"h6\" className={classes.info}>講師名：</Typography><br></br><br></br>\r\n                        <Typography variant=\"h7\" display=\"block\" className={classes.info}>料金：</Typography><br></br>\r\n                        <Typography variant=\"h7\" display=\"block\" className={classes.info}>場所：</Typography><br></br>\r\n                        <Typography variant=\"h7\" display=\"block\" className={classes.info}>日時：</Typography><br></br>\r\n                        <Typography variant=\"h7\" display=\"block\" clas